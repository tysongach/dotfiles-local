[core]
  editor = atom --wait
[alias]
  car = commit --amend --no-edit
  prom = pull --rebase origin master
[credential]
  helper = osxkeychain
[format]
  pretty = %C(green)%h%C(reset) %an (%C(blue)%ar%C(reset)) %s
[init]
  defaultBranch = main
[pull]
  ff = only
[user]
  email = tyson@tysongach.com
  name = Tyson Gach
